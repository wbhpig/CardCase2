<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/genderFragment"
    tools:context=".foreground.GenderFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F2F2F7">

        <Button
            android:id="@+id/gonext"
            style="@style/MyButtonStyle"
            android:layout_width="78dp"
            android:layout_height="30dp"
            android:layout_marginStart="284dp"
            android:layout_marginTop="52dp"
            android:background="@drawable/rounded_button"
            android:text="Save"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView300"
            android:layout_width="119dp"
            android:layout_height="20dp"
            android:layout_marginStart="146dp"
            android:layout_marginTop="60dp"
            android:text="Set Gender"
            android:gravity="center"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/backLast"
            android:layout_width="38dp"
            android:layout_height="37dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="52dp"
            android:src="@drawable/ion_chevron_back"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="100dp"
            android:orientation="vertical"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout1"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#FFFFFF">

                <ImageView
                    android:id="@+id/check1"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_marginStart="340dp"
                    android:layout_marginTop="14dp"
                    android:src="@drawable/check_01"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="150dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="15dp"
                    android:text="Male"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout2"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#FFFFFF">

                <ImageView
                    android:id="@+id/check2"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_marginStart="340dp"
                    android:layout_marginTop="14dp"
                    android:src="@drawable/check_01"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="150dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="15dp"
                    android:text="Female"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout3"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#FFFFFF">

                <ImageView
                    android:id="@+id/check3"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_marginStart="340dp"
                    android:layout_marginTop="14dp"
                    android:src="@drawable/check_01"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="150dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="15dp"
                    android:text="Non-binary"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout4"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#FFFFFF">

                <ImageView
                    android:id="@+id/check4"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_marginStart="340dp"
                    android:layout_marginTop="14dp"
                    android:src="@drawable/check_01"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="150dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="15dp"
                    android:text="Genderqueer"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layout5"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:background="#FFFFFF">

                <ImageView
                    android:id="@+id/check5"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:layout_marginStart="340dp"
                    android:layout_marginTop="14dp"
                    android:src="@drawable/check_01"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="150dp"
                    android:layout_height="17dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="15dp"
                    android:text="Transgender"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>


        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>